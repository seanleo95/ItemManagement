<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.item">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.item.ItemListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.item.AddItemActivity">
        </activity>
        <activity android:name="com.example.item.ShowItemActivity">
        </activity>
        <activity android:name="com.example.item.ItemSearch">
        </activity>
        <activity android:name="com.example.item.LeadActivity">
        </activity>
        <activity android:name="com.example.item.DeveloperActivity">
        </activity>
        <activity android:name=".NotificationResultActivity">
            <intent-filter>
                <action android:name="More"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>